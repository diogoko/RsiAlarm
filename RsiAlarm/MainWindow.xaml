<Window x:Class="RsiAlarm.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RsiAlarm"
        Height="300"
        Width="300"
        WindowStyle="None"
        ShowActivated="false"
        ShowInTaskbar="false"
        Topmost="true"
        Top="0"
        Left="0"
        Visibility="Hidden"
        IsHitTestVisible="false"
        Loaded="Window_Loaded" 
        Focusable="False" 
        IsTabStop="False" 
        ResizeMode="NoResize"
        Background="Transparent"
        AllowsTransparency="true">
    <Grid>
        <Rectangle HorizontalAlignment="Stretch" Name="WarningRectangle" Stroke="{x:Null}" VerticalAlignment="Stretch" Fill="Red" Opacity="0.0"/>
    </Grid>
</Window>
